<view class="musicList">
  <block wx:for="{{musicList}}" wx:for-item="music" wx:for-index="musicIndex" wx:key="musicKey">
    <view class="item-music">
      <image class="item-image" src="{{music.albumPic}}" />
      <view class="music-common-name">{{music.name}}</view>
      <view class="music-artistNames">{{music.artistNames}}</view>
    </view>
  </block>
</view>
